      <float name="xPosition"/>
      <float name="yPosition"/>
      <float name="scale"/>
      <float name="depth"/>
      <string name="mirrorX" values="yesno" defaultvalue="no"/>
      <string name="team" values="teams" defaultvalue="OWN_TEAM"/>
