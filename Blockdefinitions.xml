      <string name="value"/>
      <string name="adjust method" values="valueadjust" defaultvalue="set"/>
    </action>

    <action name="adjustCharacterValue">
      <string name="property" values="charactervaluesAdjustable" defaultvalue="health"/>
      <float name="value"/>
      <string name="adjust method" values="valueadjust" defaultvalue="set"/>
    </action>

    <action name="aimStickAtTarget">
      <float name="duration seconds" />
      <string name="only aim when needed" values="yesno" defaultvalue="no"/>
	  <string name="use rotation speed limits" values="yesno" defaultvalue="no"/>
	  <float name="min rotation speed per second in degrees" />
	  <float name="max rotation speed per second in degrees" />
	  <float name="use min rotation speed when angle to target under" />
	  <float name="use max rotation speed when angle to target over" />  
    </action>

    <action name="buyUpgrade">
