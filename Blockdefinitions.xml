    </action>

    <action name="stopSound">
      <string name="soundGroup"/>
    </action>
	
    <action name="pressButton">
      <string name="button" values="buttons" defaultvalue = "FACE_BOTTOM"/>
      <float name="duration in seconds"/>
    </action>

    <action name="pressStick">
