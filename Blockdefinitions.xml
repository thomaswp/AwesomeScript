      <string name="id"/>
      <string name="receive groups" values="target receive groups" multiselect="true"/>
      <string name="teams" values="teamswithtarget" multiselect="true"/>
      <float name="xOffset"/>
      <float name="yOffset"/>
      <float name="width"/>
      <float name="height"/>
	  <string name="ignore invisibility" values="yesno" defaultvalue="yes"/>
	  <string name="check line of sight" values="yesno" defaultvalue="no"/>	  
    </action>

    <action name="emitTargetInArea">
