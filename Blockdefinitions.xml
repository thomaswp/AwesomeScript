      <float name="animationYPosition"/>
      <float name="animationSize"/>
      <string name="animationNameBasedOnInputHardware" values="yesno" defaultvalue="no"/>
      <string name="animationTextsBasedOnInputHardware" values="yesno" defaultvalue="yes"/>
    </action>

    <action name="playAnimation">
