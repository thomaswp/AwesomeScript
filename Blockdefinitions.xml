      <string name="team" values="ownenemy" defaultvalue="ENEMY_TEAM"/>
    </action>

    <action name="selectTarget">
      <string name="groups to select" values="target groups" multiselect="true"/>
      <string name="teams" values="teams" multiselect="true"/>
      <string name="class"/>
	  <string name="only check parent" values="yesno" defaultvalue="no"/>
