      <string name="value" values="flagtoggle" defaultvalue="no"/>
    </action>

    <action name="setMissionBool">
      <string name="id"/>
      <string name="value" values="flagtoggle" defaultvalue="no"/>
    </action>

    <action name="spawnCharacter">
      <string name="className"/>
	  <string name="aiFileName"/>
      <string name="team" values="teamswithnumbers" defaultvalue="OWN_TEAM"/>
      <float name="count"/>
	  <string name="spawnPositionNodeID"/>
	  <float name="spawnPositionOffsetX"/>
	  <float name="spawnPositionOffsetY"/>
    </action>
	
	<action name="stopAnimation">	
